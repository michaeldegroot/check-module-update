{
  "name": "check-module-update",
  "version": "0.0.2c",
  "description": "Makes sure users stay up to date with your module!",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*",
    "coveralls": "npm bin /istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/* && node node_modules/coveralls/bin/coveralls.js < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaeldegroot/check-module-update.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaeldegroot/check-module-update/issues"
  },
  "homepage": "https://github.com/michaeldegroot/check-module-update#readme",
  "dependencies": {
    "cli-color": "^1.1.0",
    "npm-registry-client": "^7.1.2"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "assert-plus": "^1.0.0",
    "coveralls": "^2.11.12",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2"
  }
}
